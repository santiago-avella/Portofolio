---
import Chatbot from "@/components/Chatbot.astro";
import Navbar from "../components/Navbar.astro"
import App from "../layouts/App.astro"
import { supabase } from "../lib/supabase"
import { format } from "date-fns";
import ContacIcons from "@/components/ContacIcons.astro";



interface Experience {
    initial_date: Date,
    ending_date: Date,
    description: string,
    position: string, 
    company: string
}   

const { data, error} = await supabase
    .from<'experience', Experience>('experience')
    .select()
const experiences = data as Experience[]
---

<App pageTitle="Home">
   <Chatbot/>
   <Navbar/>
   <section id="section_description" class="mx-[2%] sm:mx-[7%] md:mx-[10%] lg:mx-[18%] xl:[18%] mt-24 sm:mt-36 scroll-mt-[-250px]">
        <div id="container" class="mx-auto max-w-[950px] px-2.5 sm:px-4">
            <div id="flex" class="flex flex-col sm:flex-row gap-3 sm:gap-0 sm:justify-evenly ">
                <div id="flex_child1" class="order-2 sm:order-1 sm:max-w-[50%] my-auto">
                    <p class="font-josefin text-lime-300/95 text-[110%]  sm:text-left sm:text-[130%] font-semibold leading-[170%]">
                        I`m Santiago Avella Torres, systems engineer and software developer, I work in the creation of robust, scalable and quality technological solutions.
                    </p>
                    <div class="flex items-center justify-between sm:justify-baseline sm:gap-7 mt-4 sm:mt-4.5">
                        <div id="child_button" class="sm:w-auto">
                            <a href="/downloads/SantiagoAvellaTorresResume.pdf"  download="CV_SantiagoAvella" class="inline-flex items-center justify-center p-[1.8px] sm:p-[2.2px] overflow-hidden text-[80%] sm:text-[110%] font-josefin italic font-bold text-black rounded-[10px] group bg-gradient-to-br from-greenCustom1 to-lime-300  dark:text-white dark:hover:text-gray-900">
                                <span class="relative p-2 sm:px-4.5 sm:py-1.5 transition-all ease-in duration-75 bg-greenCustom1 text-white group-hover:text-black rounded-md group-hover:bg-transparent group-hover:dark:bg-transparent">
                                    Download CV
                                </span>
                            </a>
                        </div>
                        <ContacIcons/>
                    </div>

                </div>
                <div id="flexchild2" class="order-1">
                     <img src="/imgs/me1.jpeg" style="filter: grayscale(0); transition: all 0.3s;" onmouseover="this.style.filter='grayscale(1)'" onmouseout="this.style.filter='grayscale(0)'" alt="" class="mx-auto max-h-[200px] aspect-1 object-cover object-top-left rounded-br-[8%] rounded-tl-[8%] ">
                </div>
            </div>
        </div>  
   </section>

